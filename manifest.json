{
    "manifest_version": 3,
    "name": "Activity Tracker",
    "version": "1.0",
    "description": "Track your web activity to enhance productivity.",
    "permissions": ["activeTab", "storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "options_page": "options.html"
  }